                        android:layout_toRightOf="@+id/profileSettingLassrafdd"
                        android:text="Order History"
                        android:textColor="@color/bg_tab_text_color"
                        android:textSize="@dimen/_13sdp"
                        tools:ignore="RelativeOverlap" />




                </RelativeLayout>



