                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="1dp"
                android:background="@color/menu_color"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/vender_home_btn"
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:background="@color/menu_color"
                    android:padding="0dp">


                    <LinearLayout
                        android:id="@+id/imagelayout_dashbord"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@drawable/ic_home"
                            tools:ignore="contentDescription" />

                    </LinearLayout>


                    <TextView
                        android:id="@+id/imagelayout_dashbords"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_centerVertical="true"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="10dp"
                        android:layout_toRightOf="@+id/imagelayout_dashbord"
                        android:text="Home"
                        android:textColor="@color/bg_tab_text_color"
                        android:textSize="@dimen/_13sdp" />

                </RelativeLayout>


                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/colorAccent" />

                <RelativeLayout
                    android:id="@+id/vender_menu_profile"
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:background="@color/menu_color"
                    android:padding="0dp">


                    <LinearLayout
                        android:id="@+id/manage_lay"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:src="@drawable/userlist"
                            tools:ignore="contentDescription" />

