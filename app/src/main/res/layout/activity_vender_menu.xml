<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menuBar"
    android:layout_width="match_parent"
    android:background="@color/menu_color"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:descendantFocusability="blocksDescendants">





    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="195dp">

        <ImageView
            android:id="@+id/blur_image_vender"
            android:layout_width="match_parent"
            android:layout_height="195dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:scaleType="fitXY"
            android:src="@drawable/pumpscren"></ImageView>


        <FrameLayout
            android:id="@+id/frame_register_parent"
            android:layout_width="140dp"
            android:layout_height="140dp"
            android:layout_centerInParent="true">


            <RelativeLayout
                android:id="@+id/image_background"
                android:layout_width="140dp"
                android:layout_height="140dp">

                <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                    android:id="@+id/venderuserprofile_image"
                    android:layout_width="@dimen/_140sdp"
                    android:layout_height="140dp"
                    android:layout_centerInParent="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/pumpscren"
                    app:civ_border_color="@color/white_color"
                    app:civ_border_width="2dp" />

            </RelativeLayout>


        </FrameLayout>

        <LinearLayout
            android:id="@+id/relativelayout"
            android:layout_width="wrap_content"
            android:layout_marginTop="@dimen/_30sdp"
            android:layout_toRightOf="@+id/frame_register_parent"
            android:layout_height="match_parent"
            android:gravity="center">

            <RelativeLayout
                android:id="@+id/select_img_vender"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="@dimen/_5sdp"
                android:background="@drawable/circle">

                <ImageView
                    android:layout_width="35dp"
                    android:layout_height="25dp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/ic_edit" />
            </RelativeLayout>
        </LinearLayout>

        <TextView

            android:id="@+id/vender_profilename"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/frame_register_parent"
            android:layout_marginRight="@dimen/_10sdp"
            android:gravity="center"
            android:layout_marginLeft="@dimen/_5sdp"
            android:text="Uttam Kumar"
            android:textStyle="bold"
            android:textColor="#1ade10"
            android:textSize="@dimen/_15sdp" />


    </RelativeLayout>



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <LinearLayout
