<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    xmlns:card_view="http://schemas.android.com/tools"
    android:orientation="vertical"
    tools:context=".Venderlogin.Activity.Vender_Transaction_Activity">


    <LinearLayout
        android:id="@+id/main_title_la"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:background="@color/colorPrimaryDark">

        <RelativeLayout
            android:id="@+id/forget_bolnus"
            android:layout_width="55dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:gravity="center_vertical">

            <ImageView
                android:id="@+id/btnsetting"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_alignParentLeft="true"
                android:background="@drawable/ic_arrow_back"
                tools:ignore="contentDescription"/>

        </RelativeLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:layout_weight="10"
            android:layout_marginRight="@dimen/_3sdp"
            android:background="@color/colorPrimary">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:layout_gravity="center"
                android:gravity="center">

                <ImageView
                    android:layout_width="@dimen/_35sdp"
                    android:layout_height="@dimen/_35sdp"
                    android:background="@mipmap/allforcar"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_gravity="center"
                android:gravity="left">



                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_18sdp"
                    android:layout_centerInParent="true"
                    android:text="My Account"
                    android:textColor="@color/white_color"
                    tools:ignore="RelativeOverlap" />

            </LinearLayout>






        </LinearLayout>



    </LinearLayout>

    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_below="@+id/main_title_la"
        android:layout_height="match_parent"
        android:fillViewport="true">

    <LinearLayout
        android:id="@+id/tot_del_lay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5sdp"
        android:orientation="vertical">


        <LinearLayout
            android:id="@+id/et_alreadlylogin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2"
            android:gravity="center">


            <android.support.v7.widget.CardView
                android:id="@+id/vender_product_list"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_65sdp"
                android:layout_below="@+id/news_recently_updates"
                android:layout_weight="1"
                app:cardCornerRadius="6dp"
                android:layout_margin="5dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:background="@drawable/button_background">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/vender_Completeuser"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            android:gravity="center"
                            android:textSize="20sp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textStyle="bold"
                            android:textColor="@color/white_color"/>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Completed Orders"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="16sp"
                            android:gravity="center"
                            android:textStyle="bold"
                            android:textColor="@color/white_color"/>

                    </LinearLayout>







                </RelativeLayout>

            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/myprofile"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_65sdp"
                android:layout_below="@+id/news_recently_updates"
                android:layout_weight="1"
                app:cardCornerRadius="6dp"
                android:layout_margin="5dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:background="@drawable/button_background">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/vender_registreduser"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            android:gravity="center"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="20sp"
                            android:textStyle="bold"
                            android:textColor="@color/white_color"/>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Processing Orders"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:gravity="center"
                            android:textColor="@color/white_color"/>

                    </LinearLayout>







                </RelativeLayout>

            </android.support.v7.widget.CardView>


        </LinearLayout>
        <LinearLayout
            android:id="@+id/et_alreadlylogind"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2"
            android:gravity="center">


            <android.support.v7.widget.CardView
                android:id="@+id/vender_product_lists"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_65sdp"
                android:layout_below="@+id/news_recently_updates"
                android:layout_weight="1"
                app:cardCornerRadius="6dp"
                android:layout_margin="5dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:background="@drawable/button_background">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/vender_rejectedorder"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            android:gravity="center"
                            android:textSize="20sp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textStyle="bold"
                            android:textColor="@color/white_color"/>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Rejected Orders"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="16sp"
                            android:gravity="center"
                            android:textStyle="bold"
                            android:textColor="@color/white_color"/>

                    </LinearLayout>







                </RelativeLayout>

            </android.support.v7.widget.CardView>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/et_alreadlylo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">

            <android.support.v7.widget.CardView
                android:id="@+id/myprofild"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_below="@+id/news_recently_updates"
                android:layout_weight="1"
                app:cardCornerRadius="6dp"
                android:layout_margin="5dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:background="@drawable/button_background">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/vender_totalpaymentrecive"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="20sp"
                            android:textStyle="bold"
                            android:textColor="@color/white_color"/>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Total Orders"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:gravity="center"
                            android:textColor="@color/white_color"/>

                    </LinearLayout>







                </RelativeLayout>

            </android.support.v7.widget.CardView>



        </LinearLayout>

        <LinearLayout
            android:id="@+id/et_alread"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">

            <android.support.v7.widget.CardView
                android:id="@+id/myprofildad"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_below="@+id/news_recently_updates"
                android:layout_weight="1"
                app:cardCornerRadius="6dp"
                android:layout_margin="5dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:background="@drawable/button_background">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/vender_totalbalance"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"

                            android:gravity="center"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="20sp"
                            android:textStyle="bold"
                            android:textColor="@color/white_color"/>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="My Total Balance"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:gravity="center"
                            android:textColor="@color/white_color"/>

                    </LinearLayout>







                </RelativeLayout>

            </android.support.v7.widget.CardView>



        </LinearLayout>

        <LinearLayout
            android:id="@+id/et_alreadlylod"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">

            <android.support.v7.widget.CardView
                android:id="@+id/myprofilda"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_below="@+id/news_recently_updates"
                android:layout_weight="1"
                app:cardCornerRadius="6dp"
                android:layout_margin="5dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:background="@drawable/button_background">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/vender_totalproductamt"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="20sp"
                            android:textStyle="bold"
                            android:textColor="@color/white_color"/>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Total Sale Amount"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:gravity="center"
                            android:textColor="@color/white_color"/>

                    </LinearLayout>







                </RelativeLayout>

            </android.support.v7.widget.CardView>



        </LinearLayout>




        <LinearLayout
            android:id="@+id/et_alreadlylogindd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center">

            <android.support.v7.widget.CardView
                android:id="@+id/myprofileds"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_below="@+id/news_recently_updates"
                android:layout_weight="1"
                app:cardCornerRadius="6dp"
                android:layout_margin="5dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:background="@drawable/button_background">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <TextView
                            android:id="@+id/vender_all4carscommion"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="20sp"
                            android:textStyle="bold"
                            android:textColor="@color/white_color"/>


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text="Total ALL4CARS Commision"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_centerVertical="true"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:gravity="center"
                            android:textColor="@color/white_color"/>

                    </LinearLayout>







                </RelativeLayout>

            </android.support.v7.widget.CardView>



        </LinearLayout>


    </LinearLayout>

    </ScrollView>

</RelativeLayout>